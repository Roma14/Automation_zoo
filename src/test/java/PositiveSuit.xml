<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="PositiveTests">
    <test name="PositiveTests" parallel="true" thread-count="2">
        <groups>
            <run>
                <include name="positive"/>
            </run>
        </groups>
        <classes>
            <class name="BeastTest"/>
            <class name="BirdTest"/>
            <class name="FishTest"/>
        </classes>
    </test>
</suite>